{
  "name": "pristinevault",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "format": "npx prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=test DB_NAME=test_jainsangh  jest --detectOpenHandles",
    "migrate:reset": "NODE_ENV=test DB_NAME=test_jainsangh  npx sequelize-cli db:migrate:undo:all",
    "migrate": "NODE_ENV=test DB_NAME=test_jainsangh npx sequelize-cli db:migrate",
    "pretest": "NODE_ENV=test DB_NAME=test_jainsangh  npm run migrate:reset && npm run migrate"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.32.0",
    "@google-cloud/storage": "^7.14.0",
    "aws-sdk": "^2.1123.0",
    "axios": "^1.4.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.0",
    "firebase": "^11.0.2",
    "firebase-admin": "^11.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^3.11.4",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15",
    "pg": "^8.13.1",
    "razorpay": "^2.8.6",
    "sequelize": "^6.12.5",
    "socket.io": "^4.1.2",
    "uuid": "^8.3.2",
    "vercel": "^32.3.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.3.3"
  }
}
